{
    "name": "acquia/acquia-sdk-php-cloud-db",
    "type": "library",
    "description": "Acquia Cloud database component for the Acquia SDK for PHP.",
    "keywords": ["acquia"],
    "homepage": "https://github.com/acquia/acquia-sdk-php",
    "license": "MIT",
    "authors": [
        {
            "name": "See contributors",
            "homepage": "https://github.com/acquia/acquia-sdk-php/graphs/contributors"
        }
    ],
    "repositories": [
        {
            "type": "pear",
            "url": "pear.php.net"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "acquia/acquia-sdk-php-cloud-env": "self.version",
        "pear-pear.php.net/Net_DNS2": "1.3.2"
    },
    "autoload": {
        "psr-0": {
            "Acquia\\Cloud\\Database": ""
        }
    },
    "target-dir": "Acquia/Cloud/Database"
}
